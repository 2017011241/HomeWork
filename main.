		}
		if ((HWND)lParam == BUTTON[5])
		{
			Text[6] = CreateWindow("STATIC", "请输入三角形的三条边",
				WS_TABSTOP | WS_VISIBLE | WS_CHILD | BS_LEFT, 250, 280, 200, 20,
