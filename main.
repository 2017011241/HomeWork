							FILE *fp = fopen("d:\\VS2015编程项目汇总\\GRASS\\File\\text1.txt", "w");
							fprintf(fp, "总预算为：%.0f\n总面积为：%.0f\n使用金额：%.0f\n使用面积：%.0f\n创建矩形草坪\n草坪面积为:%.0f\n建立草坪使用金额为:%.0f\n创建矩形游泳池\n泳池面积为：%.0f\n建立泳池使用金额为:%.0f\n"
								, cost.complectprice, cost.complectarea, cost.currentprice, cost.currentarea, rectangle.getarea(), rectangle.getarea()*grass.price, rectanglep.getarea(),rectanglep.getarea()*pool.price);
							fclose(fp);
						}
						if(rectangle.length != 0 && rectangle.wide != 0 &&triangle.a!=0&&triangle.b!=0&&triangle.c!=0 )
						{
							FILE *fp = fopen("d:\\VS2015编程项目汇总\\GRASS\\File\\text1.txt", "w");
							fprintf(fp, "总预算为：%.0f\n总面积为：%.0f\n使用金额：%.0f\n使用面积：%.0f\n创建矩形草坪\n草坪面积为:%.0f\n建立草坪使用金额为:%.0f\n创建三角型游泳池\n泳池面积为：%.0f\n建立泳池使用金额为:%.0f\n"
								, cost.complectprice, cost.complectarea, cost.currentprice, cost.currentarea, rectangle.getarea(), rectangle.getarea()*grass.price, trianglep.getarea(), trianglep.getarea()*pool.price);
							fclose(fp);
						}
						if(triangle.a!=0&&triangle.b!=0&&triangle.c!=0&&circlep.r!=0)
						{
							FILE *fp = fopen("d:\\VS2015编程项目汇总\\GRASS\\File\\text1.txt", "w");
							fprintf(fp, "总预算为：%.0f\n总面积为：%.0f\n使用金额：%.0f\n使用面积：%.0f\n创建三角形草坪\n草坪面积为:%.0f\n建立草坪使用金额为:%.0f\n创建圆形游泳池\n泳池面积为：%.0f\n建立泳池使用金额为:%.0f\n"
								, cost.complectprice, cost.complectarea, cost.currentprice, cost.currentarea, triangle.getarea(), triangle.getarea()*grass.price, circlep.getarea(), circlep.getarea()*pool.price);
							fclose(fp);
						}
						if(triangle.a != 0 && triangle.b != 0 && triangle.c != 0&&rectanglep.length!=0&&rectanglep.wide!=0)
						{
							FILE *fp = fopen("d:\\VS2015编程项目汇总\\GRASS\\File\\text1.txt", "w");
							fprintf(fp, "总预算为：%.0f\n总面积为：%.0f\n使用金额：%.0f\n使用面积：%.0f\n创建三角形草坪\n草坪面积为:%.0f\n建立草坪使用金额为:%.0f\n创建矩形游泳池\n泳池面积为：%.0f\n建立泳池使用金额为:%.0f\n"
								, cost.complectprice, cost.complectarea, cost.currentprice, cost.currentarea, triangle.getarea(), triangle.getarea()*grass.price, rectanglep.getarea(), rectanglep.getarea()*pool.price);
							fclose(fp);
						}
						if(triangle.a != 0 && triangle.b != 0 && triangle.c != 0 && trianglep.a!=0&&trianglep.b!=0&&trianglep.c!=0)
						{
							FILE *fp = fopen("d:\\VS2015编程项目汇总\\GRASS\\File\\text1.txt", "w");
							fprintf(fp, "总预算为：%.0f\n总面积为：%.0f\n使用金额：%.0f\n使用面积：%.0f\n创建三角形草坪\n草坪面积为:%.0f\n建立草坪使用金额为:%.0f\n创建三角形游泳池\n泳池面积为：%.0f\n建立泳池使用金额为:%.0f\n"
								, cost.complectprice, cost.complectarea, cost.currentprice, cost.currentarea, triangle.getarea(), triangle.getarea()*grass.price, trianglep.getarea(),trianglep.getarea()*pool.price);
							fclose(fp);
						}
					}
			}
			else
			{
				MessageBox(NULL, TEXT("当前金额或面积大于预算"), TEXT("提示"), MB_YESNO| MB_ICONQUESTION);
			}
		}
		if ((HWND)lParam == BUTTON[17])
		{
			GetWindowText(Edit[6], wholeprice, 64);
			GetWindowText(Edit[7], wholearea, 64);
			cost.complectprice=atof(wholeprice);
			cost.complectarea=atof(wholearea);
			SetWindowText(Edit[8],"0");
			SetWindowText(Edit[9],"0");
		}
		if ((HWND)lParam == BUTTON[2])
		{
			Text[5] = CreateWindow("STATIC", "选择草坪的形状",
				WS_TABSTOP | WS_VISIBLE | WS_CHILD | BS_LEFT, 300, 180, 200, 20,
				hwnd, NULL, (HINSTANCE)GetWindowLong(hwnd, GWL_HINSTANCE), NULL);
			BUTTON[3] = CreateWindow("BUTTON", "圆形",
				WS_TABSTOP | WS_VISIBLE | WS_CHILD | BS_DEFPUSHBUTTON, 250, 220, 30, 20,
				hwnd, NULL, (HINSTANCE)GetWindowLong(hwnd, GWL_HINSTANCE), NULL);
			BUTTON[4] = CreateWindow("BUTTON", "矩形",
				WS_TABSTOP | WS_VISIBLE | WS_CHILD | BS_DEFPUSHBUTTON, 300, 220, 30, 20,
				hwnd, NULL, (HINSTANCE)GetWindowLong(hwnd, GWL_HINSTANCE), NULL);
			BUTTON[5] = CreateWindow("BUTTON", "三角形",
				WS_TABSTOP | WS_VISIBLE | WS_CHILD | BS_DEFPUSHBUTTON, 350, 220, 50, 20,
				hwnd, NULL, (HINSTANCE)GetWindowLong(hwnd, GWL_HINSTANCE), NULL);

		}
		if ((HWND)lParam == BUTTON[3])
		{
			
			Text[6] = CreateWindow("STATIC", "请输入圆形的半径",
				WS_TABSTOP | WS_VISIBLE | WS_CHILD | BS_LEFT, 250, 280, 200, 20,
				hwnd, NULL, (HINSTANCE)GetWindowLong(hwnd, GWL_HINSTANCE), NULL);
			Edit[0] = CreateWindow("EDIT", "",
				WS_VISIBLE | WS_CHILD | WS_BORDER | DT_VCENTER, 410, 280, 100, 20,
				hwnd, NULL, (HINSTANCE)GetWindowLong(hwnd, GWL_HINSTANCE), NULL);
			Text[7] = CreateWindow("STATIC", "请选择草坪的档次",
				WS_TABSTOP | WS_VISIBLE | WS_CHILD | BS_LEFT, 250, 320, 200, 20,
				hwnd, NULL, (HINSTANCE)GetWindowLong(hwnd, GWL_HINSTANCE), NULL);
			BUTTON[6] = CreateWindow("BUTTON", "高级草",
				WS_TABSTOP | WS_VISIBLE | WS_CHILD | BS_DEFPUSHBUTTON, 220, 350, 50, 20,
				hwnd, NULL, (HINSTANCE)GetWindowLong(hwnd, GWL_HINSTANCE), NULL);
			BUTTON[7] = CreateWindow("BUTTON", "中级草",
				WS_TABSTOP | WS_VISIBLE | WS_CHILD | BS_DEFPUSHBUTTON, 270, 350, 50, 20,
				hwnd, NULL, (HINSTANCE)GetWindowLong(hwnd, GWL_HINSTANCE), NULL);
			BUTTON[8] = CreateWindow("BUTTON", "初级草",
				WS_TABSTOP | WS_VISIBLE | WS_CHILD | BS_DEFPUSHBUTTON, 320, 350, 50, 20,
				hwnd, NULL, (HINSTANCE)GetWindowLong(hwnd, GWL_HINSTANCE), NULL);
		}
		if ((HWND)lParam == BUTTON[4])
		{
			Text[6] = CreateWindow("STATIC", "请输入矩形的长和宽",
				WS_TABSTOP | WS_VISIBLE | WS_CHILD | BS_LEFT, 250, 280, 200, 20,
