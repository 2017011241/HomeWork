				WS_VISIBLE | WS_CHILD | WS_BORDER | DT_VCENTER, 410, 280, 100, 20,
				hwnd, NULL, (HINSTANCE)GetWindowLong(hwnd, GWL_HINSTANCE), NULL);
			Edit[2] = CreateWindow("EDIT", "",
				WS_VISIBLE | WS_CHILD | WS_BORDER | DT_VCENTER, 410, 310, 100, 20,
				hwnd, NULL, (HINSTANCE)GetWindowLong(hwnd, GWL_HINSTANCE), NULL);
			Text[7] = CreateWindow("STATIC", "请选择草坪的档次",
				WS_TABSTOP | WS_VISIBLE | WS_CHILD | BS_LEFT, 250, 350, 200, 20,
				hwnd, NULL, (HINSTANCE)GetWindowLong(hwnd, GWL_HINSTANCE), NULL);
			BUTTON[18] = CreateWindow("BUTTON", "高级草",
				WS_TABSTOP | WS_VISIBLE | WS_CHILD | BS_DEFPUSHBUTTON, 220, 370, 50, 20,
				hwnd, NULL, (HINSTANCE)GetWindowLong(hwnd, GWL_HINSTANCE), NULL);
			BUTTON[19] = CreateWindow("BUTTON", "中级草",
				WS_TABSTOP | WS_VISIBLE | WS_CHILD | BS_DEFPUSHBUTTON, 270, 370, 50, 20,
				hwnd, NULL, (HINSTANCE)GetWindowLong(hwnd, GWL_HINSTANCE), NULL);
			BUTTON[20] = CreateWindow("BUTTON", "初级草",
				WS_TABSTOP | WS_VISIBLE | WS_CHILD | BS_DEFPUSHBUTTON, 320, 370, 50, 20,
