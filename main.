						if (circlep.r != 0)
						{
							FILE *fp = fopen("d:\\VS2015编程项目汇总\\GRASS\\File\\text1.txt", "w");
							fprintf(fp, "总预算为：%.0f\n总面积为：%.0f\n使用金额：%.0f\n使用面积：%.0f\n当前创建圆形草坪\n草坪面积为:%f\n建立草坪使用金额为:%f\n"
								, cost.complectprice, cost.complectarea, cost.currentprice, cost.currentarea, circlep.getarea(), circlep.getarea()*grass.price);
							fclose(fp);
						}
						if (rectanglep.wide != 0 && rectanglep.length != 0)
						{
							FILE *fp = fopen("d:\\VS2015编程项目汇总\\GRASS\\File\\text1.txt", "w");
							fprintf(fp, "总预算为：%f\n总面积为：%f\n使用金额：%f\n使用面积：.0%f\n当前创建矩形草坪\n草坪面积为:%f\n建立草坪使用金额为:%f\n"
