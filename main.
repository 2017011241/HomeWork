#pragma warning(disable:4996)
#include "Grass.h"
#include "Cost.h"
#include <windows.h>
#include <tchar.h>
#include <math.h>
#include <string>
#include <stdio.h>
#include "Circle.h"
#include "Triangle.h"
#include "Rectangle.h"
#include <stdlib.h>
#include "Pool.h"
char	szClassName[] = { "First Window" };
HWND	hParent;
// LRESULT 类型是一个32位值，指的是从窗口过程或者回调函数返回的32位值 。 CALLBACK指定此函数为回调函数
Cost cost(0,0);
Circle circle;
Grass grass(0,0);
Pool pool(0,0);
RectangleM rectangle(0,0);
Triangle triangle(0,0,0);
Circle circlep(0);
Triangle trianglep(0,0,0);
RectangleM rectanglep(0,0);
LRESULT CALLBACK WindowProc(HWND hwnd, UINT uMsg, WPARAM wParam, LPARAM lParam)
{
	HDC hdc;
	PAINTSTRUCT ps;
	RECT rect;
	HINSTANCE  hInstance;
	static HWND BUTTON[30];
	static HWND Text[11];
	static HWND Edit[20];
	CHAR wholeprice[64];
	CHAR wholearea[64];
	CHAR circleradio[64];
	CHAR grassprice[64];
	CHAR grassarea[64];
	CHAR currenta[64];
	CHAR currentp[64];
	CHAR recx[64];
	CHAR recy[64];
	CHAR rectanglearea[64];
	CHAR rectangleprice[64];
	CHAR tria[64];
	CHAR trib[64];
	CHAR tric[64];
	CHAR trianglearea[64];
	CHAR triangleprice[64];
	CHAR poolarea[64];
	CHAR poolprice[64];
	switch (uMsg)
	{
	case WM_CREATE:
	{
		hInstance = ((LPCREATESTRUCT)lParam)->hInstance;
		BUTTON[0] = CreateWindow("BUTTON", "确定",
			WS_TABSTOP | WS_VISIBLE | WS_CHILD | BS_DEFPUSHBUTTON, 550, 620, 40, 20,
			hwnd, NULL, (HINSTANCE)GetWindowLong(hwnd, GWL_HINSTANCE), NULL);
		BUTTON[1] = CreateWindow("BUTTON", "取消",
			WS_TABSTOP | WS_VISIBLE | WS_CHILD | BS_DEFPUSHBUTTON, 600, 620, 40, 20,
			hwnd, NULL, (HINSTANCE)GetWindowLong(hwnd, GWL_HINSTANCE), NULL);
		Text[0] = CreateWindow("STATIC", "请输入总预算",
			WS_TABSTOP | WS_VISIBLE | WS_CHILD | BS_DEFPUSHBUTTON, 10, 10, 100, 20,
			hwnd, NULL, (HINSTANCE)GetWindowLong(hwnd, GWL_HINSTANCE), NULL);
		Edit[6] = CreateWindow("EDIT", "",
			WS_VISIBLE | WS_CHILD | WS_BORDER | DT_VCENTER, 110, 10, 60, 20,
			hwnd, NULL, (HINSTANCE)GetWindowLong(hwnd, GWL_HINSTANCE), NULL);
		Text[1] = CreateWindow("STATIC", "请输入总面积",
			WS_TABSTOP | WS_VISIBLE | WS_CHILD | BS_DEFPUSHBUTTON, 10, 30, 100, 20,
			hwnd, NULL, (HINSTANCE)GetWindowLong(hwnd, GWL_HINSTANCE), NULL);
		BUTTON[17]= CreateWindow("BUTTON", "确定",
			WS_TABSTOP | WS_VISIBLE | WS_CHILD | BS_DEFPUSHBUTTON, 10, 50, 30, 20,
			hwnd, NULL, (HINSTANCE)GetWindowLong(hwnd, GWL_HINSTANCE), NULL);
		Edit[7] = CreateWindow("EDIT", "",
			WS_VISIBLE | WS_CHILD | WS_BORDER | DT_VCENTER, 110, 30, 60, 20,
			hwnd, NULL, (HINSTANCE)GetWindowLong(hwnd, GWL_HINSTANCE), NULL);
		Text[2] = CreateWindow("STATIC", "当前以用金额为",
			WS_TABSTOP | WS_VISIBLE | WS_CHILD | BS_DEFPUSHBUTTON, 200, 10, 100, 20,
			hwnd, NULL, (HINSTANCE)GetWindowLong(hwnd, GWL_HINSTANCE), NULL);
		Edit[8] = CreateWindow("EDIT", "",
			WS_VISIBLE | WS_CHILD | WS_BORDER | DT_VCENTER, 310, 10, 60, 20,
			hwnd, NULL, (HINSTANCE)GetWindowLong(hwnd, GWL_HINSTANCE), NULL);
		Text[3] = CreateWindow("STATIC", "当前以用面积为",
			WS_TABSTOP | WS_VISIBLE | WS_CHILD | BS_DEFPUSHBUTTON, 200, 30, 100, 20,
			hwnd, NULL, (HINSTANCE)GetWindowLong(hwnd, GWL_HINSTANCE), NULL);
		Edit[9] = CreateWindow("EDIT", "",
			WS_VISIBLE | WS_CHILD | WS_BORDER | DT_VCENTER, 310, 30, 60, 20,
			hwnd, NULL, (HINSTANCE)GetWindowLong(hwnd, GWL_HINSTANCE), NULL);
		Text[4] = CreateWindow("STATIC", "草坪的建立",
			WS_TABSTOP | WS_VISIBLE | WS_CHILD | BS_LEFT, 300, 100, 200, 20,
			hwnd, NULL, (HINSTANCE)GetWindowLong(hwnd, GWL_HINSTANCE), NULL);
		BUTTON[2] = CreateWindow("BUTTON", "建立",
			WS_TABSTOP | WS_VISIBLE | WS_CHILD | BS_DEFPUSHBUTTON, 275, 140, 30, 20,
			hwnd, NULL, (HINSTANCE)GetWindowLong(hwnd, GWL_HINSTANCE), NULL);
		Text[10] = CreateWindow("STATIC", "游泳池的建立",
			WS_TABSTOP | WS_VISIBLE | WS_CHILD | BS_LEFT, 800, 100, 200, 20,
			hwnd, NULL, (HINSTANCE)GetWindowLong(hwnd, GWL_HINSTANCE), NULL);
		BUTTON[9] = CreateWindow("BUTTON", "建立",
			WS_TABSTOP | WS_VISIBLE | WS_CHILD | BS_DEFPUSHBUTTON, 810, 140, 30, 20,
			hwnd, NULL, (HINSTANCE)GetWindowLong(hwnd, GWL_HINSTANCE), NULL);
		break;
	case WM_COMMAND:
		if ((HWND)lParam == BUTTON[0])
		{
			FILE *fp=fopen("d:\\GRASS\\File\\text1.txt","w");
			if(cost.currentarea<=cost.complectarea&&cost.currentprice<=cost.complectprice)
			{
					if(grass.price!=0&&pool.price==0)
					{
						if(circle.r!=0)
						{
							FILE *fp = fopen("d:\\VS2015编程项目汇总\\GRASS\\File\\text1.txt", "w");
							fprintf(fp, "总预算为：%.0f\n总面积为：%.0f\n使用金额：%.0f\n使用面积：%.0f\n当前创建圆形草坪\n草坪面积为:%.0f\n建立草坪使用金额为:%.0f\n"
							,cost.complectprice, cost.complectarea, cost.currentprice, cost.currentarea,circle.getarea(),circle.getarea()*grass.price);
							fclose(fp);
						}
						if(rectangle.wide!=0&&rectangle.length!=0)
						{
							FILE *fp = fopen("d:\\VS2015编程项目汇总\\GRASS\\File\\text1.txt", "w");
							fprintf(fp, "总预算为：%.0f\n总面积为：%.0f\n使用金额：%.0f\n使用面积：%.0f\n当前创建矩形草坪\n草坪面积为:%.0f\n建立草坪使用金额为:%.0f\n"
								, cost.complectprice, cost.complectarea, cost.currentprice, cost.currentarea, rectangle.getarea(), rectangle.getarea()*grass.price);
							fclose(fp);
						}
						if(triangle.a!=0&&triangle.b!=0&&triangle.c!=0)
						{
							FILE *fp = fopen("d:\\VS2015编程项目汇总\\GRASS\\File\\text1.txt", "w");
							fprintf(fp, "总预算为：%.0f\n总面积为：%.0f\n使用金额：%.0f\n使用面积：%.0f\n当前创建三角形草坪\n草坪面积为:%.0f\n建立草坪使用金额为:%.0f\n"
