						{
							FILE *fp = fopen("d:\\VS2015编程项目汇总\\GRASS\\File\\text1.txt", "w");
							fprintf(fp, "总预算为：%.0f\n总面积为：%.0f\n使用金额：%.0f\n使用面积：%.0f\n创建矩形草坪\n草坪面积为:%.0f\n建立草坪使用金额为:%.0f\n创建圆形游泳池\n泳池面积为：%.0f\n建立泳池使用金额为:%.0f\n"
								, cost.complectprice, cost.complectarea, cost.currentprice, cost.currentarea, rectangle.getarea(), rectangle.getarea()*grass.price, circlep.getarea(),circlep.getarea()*pool.price);
							fclose(fp);
						}
						if(rectangle.length!=0&&rectangle.wide!=0&&rectanglep.length!=0&&rectanglep.wide!=0)
