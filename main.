							fclose(fp);
						}
						if (trianglep.a != 0 && trianglep.b != 0 && trianglep.c != 0)
						{
							FILE *fp = fopen("d:\\VS2015编程项目汇总\\GRASS\\File\\text1.txt", "w");
							fprintf(fp, "总预算为：%.0f\n总面积为：%.0f\n使用金额：%.0f\n使用面积：%.0f\n当前创建三角形草坪\n草坪面积为:%.0f\n建立草坪使用金额为:%.0f\n"
								, cost.complectprice, cost.complectarea, cost.currentprice, cost.currentarea, trianglep.getarea(),trianglep.getarea()*grass.price);
							fclose(fp);
